<AuApp>
  <AuMainHeader
    @brandLink="https://www.vlaanderen.be/nl"
    @homeRoute="index"
    @appTitle="Testomgeving Meldingsplichtige API">
    {{#if this.session.isAuthenticated}}
      <MockLogout
        @tagName={{"button"}}
        class="au-c-button au-c-button--naked">
        <AuIcon @icon="logout"/>&nbsp;Afmelden
      </MockLogout>
    {{/if}}
  </AuMainHeader>
  <AuBodyContainer
    @scroll={{true}}>
    {{outlet}}
  </AuBodyContainer>
</AuApp>
