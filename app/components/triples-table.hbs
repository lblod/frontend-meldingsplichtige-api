{{#if @triples}}
  <AuTable>
    <:header>
      <tr>
        <th>{{t "triples.subject"}}</th>
        <th>{{t "triples.predicate"}}</th>
        <th>{{t "triples.object"}}</th>
      </tr>
    </:header>
    <:body>
      {{#each @triples as |triple|}}
        <tr>
          <td>{{triple.subject}}</td>
          <td>{{triple.predicate}}</td>
          <td>{{triple.object}}</td>
        </tr>
      {{/each}}
    </:body>
  </AuTable>
{{/if}}
