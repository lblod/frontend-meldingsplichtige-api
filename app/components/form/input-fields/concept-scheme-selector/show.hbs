<label {{did-insert this.loadData}}>{{@field.label}}</label>
<PowerSelect  @disabled={{true}} @selected={{this.selected}} @options={{this.options}} @onChange={{this.updateSelection}} as |typeDossier|>
  {{typeDossier.label}}
</PowerSelect>
{{#if errors}}
  {{#each errors as |error|}}
     <span style="color: red"> {{error.resultMessage}} </span>
  {{/each}}
{{/if}}